{
  "name": "vite-plugin-dts-generator",
  "version": "0.0.1",
  "description": "Generate declaration files for virtual module",
  "author": "Ryosuke Kogure <ryo.gk55@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "virtual-module"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsup lib/index.ts --dts --clean --format cjs,esm",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "prettier": "^2.8.3",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "tsup": "^6.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  }
}
