{
  "name": "vite-plugin-virtual-dts",
  "version": "0.1.0",
  "description": "Generate declaration files for virtual module",
  "author": "Ryosuke Kogure <ryo.gk55@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryo-gk/vite-plugin-dts-generator"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "virtual-module",
    "dts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup lib/index.ts --dts --format cjs,esm",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "prettier": "^2.8.3",
    "tsup": "^6.5.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "execa": "^5.1.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  }
}
